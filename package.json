{
  "name": "starter",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "check": "tsc -b",
    "lint": "eslint --max-warnings 0",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook --coverage --verbose",
    "storybook:test:report": "nyc report --reporter=lcov --report-dir coverage/storybook",
    "storybook:test:coverage": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn storybook:build --quiet --loglevel silent && npx http-server --silent --port 6006 storybook-static\" \"wait-on http://127.0.0.1:6006 && yarn storybook:test --maxWorkers=2 && yarn storybook:test:report\"",
    "chromatic": "chromatic"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-query-devtools": "^4.29.19",
    "jotai": "^2.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@faker-js/faker": "^8.0.2",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-coverage": "^2.0.0",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-links": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/jest": "^0.1.0",
    "@storybook/react-vite": "^9.1.5",
    "@storybook/test-runner": "^0.23.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-istanbul": "^3.2.4",
    "autoprefixer": "^10.4.14",
    "axe-playwright": "^1.2.3",
    "chromatic": "^6.18.2",
    "concurrently": "^8.2.0",
    "cross-fetch": "^3.1.6",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.1.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "globals": "^16.3.0",
    "http-server": "^14.1.1",
    "jsdom": "^22.1.0",
    "msw": "^2.11.2",
    "msw-storybook-addon": "^2.0.5",
    "nyc": "^15.1.0",
    "playwright": "^1.35.1",
    "postcss": "^8.4.24",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prop-types": "^15.8.1",
    "storybook": "^9.1.5",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.2",
